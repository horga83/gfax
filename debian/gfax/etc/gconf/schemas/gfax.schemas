		</locale>

		<locale name="it">
			<long>0:mai, 1:fino al carrier, 2:sempre, 3:solo ricezzione</long>
		</locale>

		<locale name="nl">
			<long>0:nooit, 1: tot carrier, 2:altijd, 3:bij ontvangen alleen</long>
